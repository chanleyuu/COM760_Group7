<?xml version="1.0" ?>
    <robot name="group7bot">
    <!-- Create a robot chassis as Base Link -->
    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.5 0.3 0.07"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0 0.5 1.0 1.0"/>
            </material>
        </visual>
        <!-- Base pose, collision and inertial -->
        <pose>0 0 0.1 0 0 0</pose>
        <collision>
            <geometry>
                <box size="0.5 0.3 0.07"/>
            </geometry>
        </collision>
        <inertial>
        <mass value="5"/>
        <origin rpy="0 0 0" xyz="0 0 0.1"/>
        <inertia ixx="0.03954" ixy="0" ixz="0" iyy="0.10621" iyz="0" izz="0.10621"/>
        </inertial>
        <!-- caster -->
        <visual name="caster">
            <origin rpy=" 0 0 0" xyz="0.2 0 -0.05"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="black">
                <color rgba="0.05 0.05 0.05 1.0"/>
            </material>
        </visual>
        <!-- Caster collision and inertial -->
        <collision>
            <origin xyz="0.35 0 -0.05" rpy="0 0 0" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.5"/>
            <inertia ixx="0.0005" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.0005"/>
        </inertial>
    </link>
    <!-- Create wheel right -->
    <link name="right_wheel">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </visual>
        <!--right wheel collision and inertial-->
        <collision>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <inertia ixx="0.00053" ixy="0" ixz="0" iyy="0.00053" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <!-- Create wheel left -->
    <link name="left_wheel">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </visual>
        <!--left wheel collision and inertial-->
        <collision>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <inertia ixx="0.00053" ixy="0" ixz="0" iyy="0.00053" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    
    <link name="camera_link">
        <visual>
            <geometry>
                <box size="0.010 0.03 0.03"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>
    
    <link name="camera_link_optical"></link>
    
    <link name="laser_sensor">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.1"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder raduis="0.05" length="0.1"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0.0015" ixy="0" ixz="0" iyy="0.0015" iyz="0" izz="0.0012"/>
        </inertial>
    </link>
    <!-- Joint for right wheel -->
    <joint name="joint_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="-0.05 0.15 0"/>
        <child link="right_wheel"/>
        <parent link="base_link"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <dynamics damping="1.0" friction="1.0" />
    </joint>
    <!-- Joint for left wheel -->
    <joint name="joint_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="-0.05 -0.15 0"/>
        <child link="left_wheel"/>
        <parent link="base_link"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <dynamics damping="1.0" friction="1.0" />
    </joint>
    
    <joint name="camera_joint" type="fixed">
        <parent link="base_link"/>
        <child link="camera_link"/>
        <origin xyz="0.25 0 0" rpy="0 0 0"/>
    </joint>
    
    <joint name="camera_optical_joint" type="fixed">
        <parent link="camera_link"/>
        <child link="camera_link_optical"/>
        <origin xyz="0 0 0" rpy="-1.57 0 1.57"/>
    </joint>
    
    <joint name="joint_laser_sensor" type="fixed">
         <origin rpy="0 0 0" xyz="0.15 0 0.05"/>
         <parent link="base_link"/>
         <child link="laser_sensor"/>
    </joint>
    
</robot>